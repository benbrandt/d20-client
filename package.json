{
  "author": "Ben Brandt",
  "name": "d20-client",
  "version": "0.1.0",
  "repository": "https://github.com/benbrandt/d20-client",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "start": "NODE_ENV=development TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack-dev-server -d --mode=development",
    "build": "NODE_ENV=production TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack --mode=production",
    "test": "wasm-pack test --node",
    "prettier": "prettier --write .",
    "lint": "eslint --ext .ts --ext .js ./js/**/*",
    "tsc": "tsc"
  },
  "browserslist": [
    "Chrome >= 57",
    "Edge >= 16",
    "Firefox >= 52",
    "Opera >= 44",
    "Safari >= 11",
    "Android >= 57",
    "ChromeAndroid >= 57",
    "FirefoxAndroid >= 52",
    "iOS >= 11",
    "Samsung >= 7",
    "OperaMobile >= 46",
    "not < 0.25%",
    "not dead"
  ],
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2019,
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript",
      "plugin:prettier/recommended",
      "prettier/@typescript-eslint"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.11.10",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/terser-webpack-plugin": "^4.2.0",
    "@types/webpack": "^4.41.22",
    "@types/webpack-bundle-analyzer": "^3.8.0",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/workbox-webpack-plugin": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^3.10.1",
    "@wasm-tool/wasm-pack-plugin": "^1.3.1",
    "babel-loader": "^8.1.0",
    "caniuse-lite": "^1.0.30001148",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.1.2",
    "regenerator-runtime": "^0.13.7",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "workbox-webpack-plugin": "^5.1.4"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.12.1",
    "@sentry/browser": "^5.26.0",
    "fast-text-encoding": "^1.0.3",
    "register-service-worker": "^1.7.1",
    "spectre.css": "^0.5.9"
  }
}
