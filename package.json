{
  "author": "Ben Brandt",
  "name": "d20-client",
  "version": "0.1.0",
  "repository": "https://github.com/benbrandt/d20-client",
  "license": "(MIT OR Apache-2.0)",
  "scripts": {
    "start": "NODE_ENV=development TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack-dev-server -d --mode=development",
    "build": "NODE_ENV=production TS_NODE_PROJECT=\"webpack.tsconfig.json\" webpack --mode=production",
    "test": "wasm-pack test --node",
    "prettier": "prettier --write .",
    "lint": "eslint --ext .ts --ext .js ./js/**/*",
    "tsc": "tsc"
  },
  "browserslist": [
    "Chrome >= 57",
    "Edge >= 16",
    "Firefox >= 52",
    "Opera >= 44",
    "Safari >= 11",
    "Android >= 57",
    "ChromeAndroid >= 57",
    "FirefoxAndroid >= 52",
    "iOS >= 11",
    "Samsung >= 7",
    "OperaMobile >= 46",
    "not < 0.25%",
    "not dead"
  ],
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 2019,
      "project": "./tsconfig.json",
      "sourceType": "module"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:import/typescript",
      "plugin:prettier/recommended"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@babel/runtime": "^7.16.3",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.10",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.4",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^4.5.0",
    "@types/workbox-webpack-plugin": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@wasm-tool/wasm-pack-plugin": "^1.6.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-polyfill-corejs3": "^0.4.0",
    "babel-plugin-polyfill-regenerator": "^0.3.0",
    "caniuse-lite": "^1.0.30001283",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "core-js": "^3.19.2",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.2.0",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.5",
    "node-sass": "^6.0.1",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.1",
    "preload-webpack-plugin": "^3.0.0-beta.4",
    "prettier": "^2.5.0",
    "regenerator-runtime": "^0.13.9",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "workbox-webpack-plugin": "^6.4.1"
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.19.2",
    "@sentry/browser": "^6.15.0",
    "fast-text-encoding": "^1.0.3",
    "register-service-worker": "^1.7.2",
    "spectre.css": "^0.5.9"
  }
}
